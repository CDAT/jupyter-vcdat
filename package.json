{
	"name": "jupyter-react-ext",
	"version": "0.1.0",
	"description": "Learning to make extensions for JupyterLab and incorporating React.",
	"keywords": [
		"jupyter",
		"jupyterlab",
		"jupyterlab-extension"
	],
	"homepage": "https://github.com/downiec/jupyter-react-ext.git",
	"bugs": {
		"url": "https://github.com/downiec/jupyter-react-ext.git/issues"
	},
	"license": "BSD-3-Clause",
	"author": "Carlos Downie",
	"files": [
		"lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff,woff2,ttf}",
		"style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff,woff2,ttf}"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/downiec/jupyter-react-ext.git"
	},
	"scripts": {
		"build": "tsc",
		"clean": "rimraf lib",
		"watch": "tsc -w",
		"link": "jupyter labextension link . --nobuild",
		"prepare": "npm run clean && npm run build",
		"scss-watch": "node-sass --watch style/scss -o style/css",
		"scss": "node-sass style/scss -o style/css"
	},
	"dependencies": {
		"@jupyterlab/application": "^0.18.6",
		"@jupyterlab/apputils": "^0.18.4",
		"@jupyterlab/cells": "^0.18.4",
		"@jupyterlab/console": "^0.18.4",
		"@jupyterlab/filebrowser": "^0.18.6",
		"@phosphor/commands": "1.6.1",
		"@phosphor/coreutils": "^1.3.0",
		"@phosphor/messaging": "^1.2.2",
		"@phosphor/widgets": "^1.6.0",
		"@types/react-dom": "^16.0.7",
		"bootstrap": "^4.1.3",
		"jquery": "^3.3.1",
		"npm": "^6.4.1",
		"popper.js": "^1.14.4",
		"react-bootstrap": "^0.32.4",
		"react-dom": "^16.5.2",
		"react-toastify": "^4.3.2",
		"vcs-widgets": "github:uv-cdat/vcs-widgets"
	},
	"devDependencies": {
		"node-sass": "^4.9.3",
		"nodemon": "^1.18.4",
		"rimraf": "^2.6.1",
		"typescript": "^2.9.2"
	},
	"jupyterlab": {
		"extension": true
	}
}
