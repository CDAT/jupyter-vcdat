{
  "name": "jupyter-react-ext",
  "version": "0.1.0",
  "description": "Learning to make extensions for JupyterLab and incorporating React.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/downiec/jupyter-react-ext.git",
  "bugs": {
    "url": "https://github.com/downiec/jupyter-react-ext.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Carlos Downie",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/downiec/jupyter-react-ext.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "link": "jupyter labextension link . --nobuild",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/apputils": "^0.18.4",
    "@jupyterlab/cells": "^0.18.4",
    "@jupyterlab/console": "^0.18.4",
    "@jupyterlab/filebrowser": "^0.18.6",
    "@phosphor/commands": "1.6.1",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "@types/react-dom": "^16.0.7",
    "npm": "^6.4.1",
    "react-dom": "^16.5.1",
    "vcs-widgets": "uv-cdat/vcs-widgets"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "^2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
