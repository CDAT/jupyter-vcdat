{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": ["log", "debug", "info", "time", "timeEnd", "trace"]
    },
    "ordered-imports": [true, {
      "import-sources-order": "any",
      "named-imports-order": "case-insensitive"
    }],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}
